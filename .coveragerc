[run]
source = src, config, main.py, working_main.py, jax_hybrid_core.py
omit = 
    */tests/*
    */__pycache__/*
    .backup_archive/*
    backups/*
    build/*
    dist/*
    *.egg-info/*
    venv/*
    env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml